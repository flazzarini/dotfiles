# Standard bash color definitions
ESC="\[\e["
GREEN="${ESC}01;32m\]"
RED="${ESC}01;31m\]"
BLUE="${ESC}01;34m\]"
CYAN="${ESC}01;36m\]"
PURPLE="${ESC}01;35m\]"
WHITE="${ESC}01;37m\]"
YELLOW="${ESC}01;33m\]"
RESET="\[\033[0m\]"

# 256 Color picks
YELLOW142="${ESC}38;05;142m\]"
GREEN113="${ESC}38;05;113m\]"
GREEN112="${ESC}38;05;112m\]"
GRAY242="${ESC}38;05;242m\]"
GRAY247="${ESC}38;05;247m\]"

# PS1 Configurations
BAR=${GRAY242}

# vim: set ft=bash
